{
  "name": "@sikur/enums",
  "version": "1.0.0",
  "description": "This package provides TypeScript enums for Sikur's applications.",
  "types": "dist/esm/index.d.ts",
  "author": "",
  "license": "ISC",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc -b tsconfig.cjs.json tsconfig.esm.json && tsc-alias -p tsconfig.cjs.json && tsc-alias -p tsconfig.esm.json && node ./scripts/package-types.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint-dry": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint-report": "eslint \"{src,apps,libs,test}/**/*.ts\" -f checkstyle -o eslint_report.xml"
  },
  "devDependencies": {
    "@ultra/eslint-config": "^1.3.6",
    "@ultra/typescript-config": "^1.1.0",
    "eslint-formatter-checkstyle": "^8.40.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.8.3"
  }
}
